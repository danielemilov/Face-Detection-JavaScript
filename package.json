{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm install --build-from-source=canvas"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }